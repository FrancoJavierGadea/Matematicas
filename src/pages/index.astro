import * as md from '@docs/test.md';

import Tabs from '@layouts/Tabs.astro';
import GeogebraEmbed from '@layouts/GeogebraEmbed/GeogebraEmbed.astro';

---

<Layout title="Welcome to Astro.">


	<div class="Graph">

		<!-- <Graph client:load /> -->

		

	</div>

	<GeogebraEmbed id="zhpqg6md" enableShiftDragZoom showFullScreenButton allowStyleBar enableRightClick />

	
